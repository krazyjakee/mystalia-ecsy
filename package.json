{
  "name": "mystalia-ecsy",
  "author": "krazyjakee@gmail.com",
  "version": "0.0.1",
  "scripts": {
    "server": "nodemon --config configs/debugServer.nodemon.json",
    "client": "nodemon --config configs/debugClient.nodemon.json",
    "start": "concurrently \"npm:server\" \"npm:client\"",
    "start:prod": "ts-node -r tsconfig-paths/register ./src/server/index.ts",
    "build": "cross-env NODE_ENV=production webpack --config src/server/webpack.config.ts",
    "storybook": "start-storybook -s ./ -p 6006",
    "build-storybook": "build-storybook",
    "test": "npm run test:maps && jest --forceExit",
    "test:watch": "jest --watch",
    "test:maps": "ts-node -r tsconfig-paths/register src/scripts/testMaps.ts",
    "test:maps:watch": "nodemon --config configs/testMaps.nodemon.json",
    "lint:maps": "ts-node -r tsconfig-paths/register src/scripts/lintMaps.ts",
    "db:migrate": "migrate-mongo up",
    "db:rollback": "migrate-mongo down",
    "tsc": "tsc --noEmit --jsx react"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
